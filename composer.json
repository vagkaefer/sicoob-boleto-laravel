{
  "name": "vagkaefer/sicoob-boleto-laravel",
  "description": "A laravel package to work with Sicoob Bills (Boletos)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vagner Kaefer",
      "email": "vagner@kaefer.eng.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "VagKaefer\\Sicoob\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VagKaefer\\Sicoob\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VagKaefer\\Sicoob\\BoletoServiceProvider"
      ],
      "aliases": {
        "Boleto": "VagKaefer\\Sicoob\\Facades\\Boleto"
      }
    }
  },
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.0",
    "guzzlehttp/guzzle": "^7.4",
    "ramsey/uuid": "^4.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^7.0"
  }
}
